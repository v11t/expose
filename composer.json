{
    "name": "beyondcode/expose",
    "type": "project",
    "description": "Create public URLs for local sites through any firewall and VPN.",
    "keywords": [
        "expose",
        "tunnel",
        "ngrok"
    ],
    "homepage": "https://sharedwithexpose.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Marcel Pociot",
            "email": "marcel@beyondco.de"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "beyondcode/expose-common": "dev-main",
        "cboden/ratchet": "dev-master",
        "chillerlan/php-qrcode": "^5.0",
        "illuminate/database": "^11.5",
        "laravel-zero/phar-updater": "^1.3",
        "laravel/prompts": "^0.3.0@dev",
        "nunomaduro/termwind": "^2.0",
        "ratchet/pawl": "dev-master",
        "ratchet/rfc6455": "dev-master",
        "react/http": "^1.1.0",
        "react/socket": "^1.6",
        "react/stream": "^1.1.1",
        "riverline/multipart-parser": "^2.0",
        "symfony/expression-language": "^5.2",
        "symfony/http-kernel": "^7.0",
        "symfony/psr-http-message-bridge": "^7.0",
        "symfony/yaml": "^7.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "guzzlehttp/guzzle": "^7.5",
        "guzzlehttp/psr7": "^1.7|^2.0",
        "illuminate/http": "^11.5",
        "illuminate/log": "^11.5",
        "illuminate/pipeline": "^11.5",
        "illuminate/validation": "^11.5",
        "illuminate/view": "^11.5",
        "laminas/laminas-http": "^2.13",
        "laravel-zero/framework": "^11.0",
        "mockery/mockery": "^1.4.2",
        "nikic/php-parser": "^v4.10",
        "nyholm/psr7": "^1.3",
        "octoper/cuzzle": "dev-master",
        "phpunit/phpunit": "^10.5.1",
        "react/async": "^4.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Expose\\Client\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/beyondcode/pawl"
        },
        {
            "type": "git",
            "url": "https://github.com/beyondcode/RFC6455"
        },
        {
            "type": "git",
            "url": "https://github.com/beyondcode/Ratchet"
        },
        {
            "type": "git",
            "url": "https://github.com/beyondcode/cuzzle"
        },
        {
            "type": "git",
            "url": "https://github.com/beyondcode/expose-common"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "builds/expose"
    ],

    "scripts": {
        "post-create-project-cmd": [
            "@php application app:rename"
        ]
    }
}
